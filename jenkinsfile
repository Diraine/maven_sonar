     pipeline {
        agent none
        stages {
         
          stage("build & sonarqub Analysis") {
            agent any
            steps {
               def scannerHome = tool 'SonarQube Scanner'
              withSonarQubeEnv('sonar') {
               sh "${scannerHome}/bin/sonar-scanner"
                sh 'mvn clean package sonar:sonar'
              }
            }
          }
        }
      }
